<div id="password-generator-container">
  <h2>Password Generator</h2>
  <form id="password-form">
    <label for="length">Password Length:</label>
    <input type="number" id="length" name="length" min="6" max="128" >
      <button type="button" id="generate-btn">Generate Password</button>
  </form>
  <div id="generated-password">
    <strong>Your Password:</strong> <span id="password-output"></span>
  </div>
</div>
